snippet link "Link to something"
[${1:${VISUAL:Text}}](${3:http://${2:www.url.com}})$0
endsnippet

snippet img "Image"
![${1:pic alt}](${2:path}${3/.+/ "/}${3:opt title}${3/.+/"/})$0
endsnippet

snippet :down: "in"
⬇
endsnippet

snippet :up: "in"
⬆
endsnippet

snippet in "⬇ in"
⬇ in
endsnippet

snippet out "⬆ out"
⬆ out
endsnippet

# vim:ft=snippets:
